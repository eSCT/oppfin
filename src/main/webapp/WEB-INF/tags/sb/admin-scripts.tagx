<jsp:root xmlns:c="http://java.sun.com/jsp/jstl/core"
	xmlns:fn="http://java.sun.com/jsp/jstl/functions"
	xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:spring="http://www.springframework.org/tags" version="2.0">

	<jsp:output omit-xml-declaration="yes" />
	
	<!-- http://vitalets.github.io/x-editable/ -->
  <c:set var="xeditableVersion" value="1.5.0"/>
	<spring:url
		value="http://cdnjs.cloudflare.com/ajax/libs/x-editable/${xeditableVersion }/bootstrap3-editable/css/bootstrap-editable.css"
		var="xeditable_css_url" />
	<spring:url
		value="http://cdnjs.cloudflare.com/ajax/libs/x-editable/${xeditableVersion }/bootstrap3-editable/js/bootstrap-editable.min.js"
		var="xeditable_js_url" />
	
	 <link rel="stylesheet" type="text/css" href="${xeditable_css_url}" />	
	<script src="${xeditable_js_url}" type="text/javascript"><!-- required for FF3 and Opera --></script>

</jsp:root>