<html xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:c="http://java.sun.com/jsp/jstl/core"
	xmlns:spring="http://www.springframework.org/tags"
	xmlns:util="urn:jsptagdir:/WEB-INF/tags/util">
<jsp:output omit-xml-declaration="yes" />
<jsp:output doctype-root-element="HTML"
	doctype-system="about:legacy-compat" />

<jsp:directive.page contentType="text/html;charset=UTF-8" />
<jsp:directive.page pageEncoding="UTF-8" />

<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<!-- <meta http-equiv="X-UA-Compatible" content="IE=8" />  -->

<util:load-scripts />

<title>${title}</title>
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
</head>
<body>
	<div id="wrap">
		<jsp:include page="/WEB-INF/partials/header.jspx" />
		<div id="main-content" role="main">

			<div class="row" id="column-top">
				<jsp:include page="/WEB-INF/partials/searchbar.jspx" />
			</div>

			<div class="row container" id="main-search-content">
				<jsp:include page="/WEB-INF/views/${view}.jspx" />
				<p>${exception}</p>
			</div>
		</div>
		<div id="push">
			<!-- push -->
		</div>
	</div>
	<jsp:include page="/WEB-INF/partials/footer.jspx" />
</body>
</html>
